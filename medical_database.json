  "decision_trees": {
    "respiratory_symptoms": {
      "initial_question": "Do you have fever?",
      "branches": {
        "yes": {
          "question": "Do you have productive cough with green/yellow sputum?",
          "branches": {
            "yes": {
              "likely_conditions": [
                "pneumonia",
                "bronchitis"
              ]
            },
            "no": {
              "question": "Do you have muscle aches and fatigue?",
              "branches": {
                "yes": {
                  "likely_conditions": [
                    "influenza"
                  ]
                },
                "no": {
                  "likely_conditions": [
                    "common_cold",
                    "viral_infection"
                  ]
                }
              }
            }
          }
        },
        "no": {
          "question": "Do you have wheezing or difficulty breathing?",
          "branches": {
            "yes": {
              "likely_conditions": [
                "asthma",
                "allergic_reaction"
              ]
            },
            "no": {
              "likely_conditions": [
                "common_cold",
                "allergies"
              ]
            }
          }
        }
      }
    },
    "chest_pain": {
      "initial_question": "Is the chest pain crushing or squeezing?",
      "branches": {
        "yes": {
          "question": "Does it radiate to arm, neck, or jaw?",
          "branches": {
            "yes": {
              "likely_conditions": [
                "heart_attack"
              ],
              "urgency": "emergency"
            },
            "no": {
              "likely_conditions": [
                "angina",
                "heart_disease"
              ]
            }
          }
        },
        "no": {
          "question": "Does the pain worsen with breathing?",
          "branches": {
            "yes": {
              "likely_conditions": [
                "pleurisy",
                "pneumonia"
              ]
            },
            "no": {
              "likely_conditions": [
                "muscle_strain",
                "anxiety"
              ]
            }
          }
        }
      }
    }
  }
